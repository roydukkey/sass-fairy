{
	"version": "0.2",
	"ignorePaths": [
		"node_modules",
		".git",
		".jest",
		".docusaurus",
		"**/.DS_Store",
		"*.lock.json",
		"*-lock.json",
		"*.lock",
		"*.tgz",
		"*.svg"
	],
	"dictionaries": [
		"css",
		"fonts",
		"typescript"
	],
	// Before adding a word, see if there is already a dictionary in which it is contained.
	// http://github.com/streetsidesoftware/cspell-dicts
	"words": [
		"arglist",
		"browserconfig",
		"editorconfig",
		"fonticons",
		"npmignore",
		"roydukkey",
		"silvenon",
		"streetsidesoftware",
		"viewports"
	]
}
