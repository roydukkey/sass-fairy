{
	"version": "0.2",
	"ignorePaths": [
		"node_modules",
		".git",
		".jest",
		".docusaurus",
		"website/build",
		"**/.DS_Store",
		"*{.lock?(.json),-lock.{json,yaml}}",
		"*.tgz",
		"*.svg"
	],
	"dictionaries": [
		"css",
		"fullstack",
		"fonts",
		"html",
		"npm",
		"typescript"
	],
	// Before adding a word, see if there is already a dictionary in which it is contained.
	// http://github.com/streetsidesoftware/cspell-dicts
	"words": [
		"arglist",
		"atrule",
		"browserconfig",
		"browserslist",
		"clsx",
		"docsearch",
		"editorconfig",
		"factorialization",
		"factorialize",
		"fonticons",
		"globby",
		"hexcode",
		"metastring",
		"mstile",
		"nprogress",
		"preinstall",
		"roydukkey",
		"streetsidesoftware",
		"svgr",
		"tsdoc",
		"typecheck",
		"unifiedjs",
		"viewports",
		"wcag"
	]
}
